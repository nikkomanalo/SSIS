                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT CS.RequisitionId, CS.RequisitionSourceCd, CS.EmailId, CASE  WHEN CS_ODS.[RequisitionId] IS NULL THEN 'Insert' ELSE 'Update' END as TypeRecord
FROM [EmpRef].[ApplicantStatus] CS WITH(NOLOCK)
INNER JOIN [EmpRef].[CodeDetail] CD WITH(NOLOCK)
ON CS.[RequisitionSourceCd] = CD.[CodeTxt]
AND CD.[CategoryNbr] = 2
INNER JOIN ##SearchCorrectPeopleKey pp
ON CS.EmailId = PP.EmailStage
LEFT JOIN [2761_EnterpriseODS].[EmpRef].[ApplicantStatus] CS_ODS WITH(NOLOCK)
ON (CS.[RequisitionId] = CS_ODS.[RequisitionId]
AND CS.[RequisitionSourceCd] = CS_ODS.[RequisitionSourceCd]
AND PP.PeopleKey = CS_ODS.[PeopleKey])
WHERE CS_ODS.[RequisitionId] IS NULL
OR (CS.[RequisitionId] = CS_ODS.[RequisitionId]
AND CS.[RequisitionSourceCd] = CS_ODS.[RequisitionSourceCd]
AND PP.[PeopleKey] = CS_ODS.[PeopleKey]
AND CS.[LastModifiedDttm] &gt; CS_ODS.[LastModifiedDttm])</property>
