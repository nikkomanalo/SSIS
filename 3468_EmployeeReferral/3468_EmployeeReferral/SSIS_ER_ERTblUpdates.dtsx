AND ((RQ.[RequisitionId]= RQ_ODS.[RequisitionId]
	AND RQ.[RequisitionSourceCd] = RQ_ODS.[RequisitionSourceCd]
	AND RQ.[LastModifiedDttm] &gt; RQ_ODS.[LastModifiedDttm])
	OR (RQ_ODS.[RequisitionId] is null
	AND RQ_ODS.[RequisitionSourceCd] is null))
GROUP BY RQ.RequisitionId, RQ.RequisitionSourceCd, RQ_ODS.[RequisitionId]</property>
