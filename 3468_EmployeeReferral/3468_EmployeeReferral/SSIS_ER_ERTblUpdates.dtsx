                  SQLTask:Connection="{DDCE1E0E-13FA-48DA-9788-99C0B9EE82E5}"
                  SQLTask:SqlStatementSource="DECLARE @requisition_id NVARCHAR(20),&#xA;&#x9;&#x9;@system_source_cd NVARCHAR(20),&#xA;&#x9;&#x9;@LocationDesc varchar(8000),&#xA;&#x9;&#x9;@IntJobDescURL varchar(255),&#xA;&#x9;&#x9;@IntReferURL varchar(255),&#xA;&#x9;&#x9;@ExtURL varchar(255),&#xA;&#x9;&#x9;@IntJobDescURLAba varchar(255),&#xA;&#x9;&#x9;@IntReferURLAba varchar(255),&#xA;&#x9;&#x9;@ExtURLAbaInd varchar(255),&#xA;&#x9;&#x9;@ExtURLAbaPh varchar(255),&#xA;&#x9;&#x9;@IntJobDescOIPURL varchar(255),&#xA;&#x9;&#x9;@CandidateSourceCd NVARCHAR(20)&#xA;&#xA;Set @requisition_id = ?&#xA;Set @system_source_cd = ?&#xA;Set @LocationDesc = ?&#xA;Set @IntJobDescURL = ?&#xA;Set @IntReferURL = ?&#xA;Set @ExtURL = ?&#xA;Set @IntJobDescURLAba = ?&#xA;Set @IntReferURLAba = ?&#xA;Set @ExtURLAbaInd = ?&#xA;Set @ExtURLAbaPh = ?&#xA;Set @IntJobDescOIPURL = ?&#xA;Set @CandidateSourceCd = ?&#xA;&#xA;BEGIN TRY&#xA;&#xA;INSERT INTO [2761_EnterpriseODS].[EmpRef].[Requisition]&#xA;&#x9;   ([RequisitionId]&#xA;&#x9;   ,[RequisitionSourceCd]&#xA;&#x9;   ,[SystemSourceDesc]&#xA;&#x9;   ,[RequisitionTitle]&#xA;&#x9;   ,[RequisitionType]&#xA;&#x9;   ,[CountryCd]&#xA;&#x9;   ,[CountryDesc]&#xA;&#x9;   ,[LevelCd]&#xA;&#x9;   ,[LevelDesc]&#xA;&#x9;   ,[WorkForceCd]&#xA;&#x9;   ,[WorkForceDesc]&#xA;&#x9;   ,[WorkGroupCd]&#xA;&#x9;   ,[WorkGroupDesc]&#xA;&#x9;   ,[SpecialityCd]&#xA;&#x9;   ,[SpecialityDesc]&#xA;&#x9;   ,[CityCd]&#xA;&#x9;   ,[CityDesc]&#xA;&#x9;   ,[Job1Desc]&#xA;&#x9;   ,[Job2Desc]&#xA;&#x9;   ,[Qualification1Desc]&#xA;&#x9;   ,[Qualification2Desc]&#xA;&#x9;   ,[LocationDesc]&#xA;&#x9;   ,[RequisitionStatusDesc]&#xA;&#x9;   ,[RequisitionStatusDetailDesc]&#xA;&#x9;   ,[SourcingRequestStatusDesc]&#xA;&#x9;   ,[InternalJobDescURL]&#xA;&#x9;   ,[InternalReferURL]&#xA;&#x9;   ,[ExternalURL]&#xA;&#x9;   ,[LastModifiedDttm]&#xA;&#x9;   ,[RecruiterNm]&#xA;&#x9;   ,[CareerTrackCd]&#xA;&#x9;   ,[CareerTrackDesc]&#xA;&#x9;   ,[TalentSegmentCd] &#xA;&#x9;   ,[TalentSegmentDesc] &#xA;&#x9;