                  SQLTask:SqlStatementSource="DECLARE @Requisition_Id VARCHAR(20), &#xA;&#x9;@Requisition_source_cd VARCHAR(20), &#xA;&#x9;@EmailId varchar(100),&#xA;&#x9;@Candidate_source_cd VARCHAR(20)&#xA;&#xA;Set @Requisition_Id = ?&#xA;Set @Requisition_source_cd = ?&#xA;Set @EmailId = ?&#xA;Set @Candidate_source_cd = ?&#xA;&#xA;BEGIN TRY&#xA;&#xA; INSERT [2761_EnterpriseODS].[EmpRef].[ApplicantStatus]&#xA;           ([RequisitionId]&#xA;           ,[RequisitionSourceCd]&#xA;           ,[PeopleKey]&#xA;           ,[SystemSourceDesc]&#xA;           ,[ATASEmployeeStepDesc]&#xA;           ,[ApplicationStatusDesc]&#xA;           ,[OriginalAppStatusDesc]&#xA;           ,[RejectionReasonDesc]&#xA;           ,[AppliedDt]&#xA;           ,[LastModifiedDttm]&#xA;           ,[CreateUserId]&#xA;           ,[CreateDttm]&#xA;           ,[UpdateUserId]&#xA;           ,[UpdateDttm])&#xA;  SELECT CS.[RequisitionId]&#xA;     ,CS.[RequisitionSourceCd]&#xA;     ,PP.[PeopleKey] AS PeopleKey&#xA;     ,CD.DecodeTxt as SystemSourceDesc&#xA;     ,CS.[ATASEmployeeStepDesc]&#xA;     ,CONVERT(VARCHAR(50)&#xA;     ,CASE [CD].[DecodeTxt] WHEN 'ATAS' THEN&#xA;&#x9;&#x9;&#x9;CASE CS.[ATASEmployeeStatusDes]&#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'Candidate Reject - Not Applicant' THEN &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CASE CS.[RejectionReasonDesc]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Moved to Hiring Requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Matched to new Fiscal Year requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 'Not Selected'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'Candidate Reject — No Business Need' THEN &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CASE CS.[RejectionReasonDesc]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Moved to Hiring Requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Matched to new Fiscal Year requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 'Not Selected'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END  &#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'Candidate Rejected' THEN &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CASE CS.[RejectionReasonDesc]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Moved to Hiring Requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Matched to new Fiscal Year requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 'Not Selected'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END  &#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'Candidate Withdrew' THEN 'Withdrew'&#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'Business Case Candidate Rejected' THEN 'Not Selected'&#xA;&#x9;&#x9;&#x9;&#x9;ELSE CSM.NewReferralStatusDesc&#xA;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;ELSE CSM.AbacusReferralStatusDesc&#xA;&#x9; END) AS ApplicationStatusDesc&#xA;     ,CS.[ATASEmployeeStatusDes]&#xA;     ,CS.[RejectionReasonDesc]&#xA;     ,CS.[AppliedDt]&#xA;     ,CS.[LastModifiedDttm]&#xA;     ,CS.[CreateUserId]&#xA;     ,CS.[CreateDttm]&#xA;     ,CS.[UpdateUserId]&#xA;     ,CS.[UpdateDttm]&#xA;  FROM (SELECT * &#xA;    FROM [EmpRef].[ApplicantStatus] WITH(NOLOCK)&#xA;    WHERE RequisitionId = @Requisition_Id&#xA;    AND RequisitionSourceCd = @Requisition_source_cd&#xA;    AND EmailId = @EmailId) CS&#xA;  LEFT JOIN [EmpRef].[CandidateStatusMapping] CSM WITH(NOLOCK)&#xA;  ON CS.[ATASEmployeeStatusDes] = CSM.[ATASReferralStatusDesc]&#xA;  INNER JOIN [EmpRef].[CodeDetail] CD WITH(NOLOCK)&#xA;  ON CS.[RequisitionSourceCd] = CD.[CodeTxt]&#xA;  AND CD.[CategoryNbr] = 2&#xA;  INNER JOIN ##SearchCorrectPeopleKey pp&#xA;  ON CS.EmailId = PP.EmailStage&#xA;  LEFT JOIN [2761_EnterpriseODS].[EmpRef].[ApplicantStatus] CS_ODS WITH(NOLOCK)&#xA;  ON CS.[RequisitionId] = CS_ODS.[RequisitionId]&#xA;    AND CS.[RequisitionSourceCd] = CS_ODS.[RequisitionSourceCd]&#xA;    AND PP.PeopleKey = CS_ODS.[PeopleKey]&#xA;  WHERE CS_ODS.[RequisitionId] IS NULL&#xA;    AND CS_ODS.[RequisitionSourceCd] IS NULL&#xA;&#xA;&#xA;END TRY&#xA;BEGIN CATCH&#xA;&#xA;&#x9;INSERT INTO ##ErrorTable ([CandidateSourceCd], [TableName], [ErrorDesc])&#xA;&#x9;SELECT @Candidate_source_cd, 'ApplicantStatus', 'SSIS_ER_ERTblUpdates. Insert ApplicantStatus Failed; requisitonId: ' + @Requisition_Id + '; RequisitonSourceCd: ' + @Requisition_source_cd + '; EmailId: ' + @EmailId + '; DescError: ' + ERROR_MESSAGE()&#xA;&#xA;END CATCH&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
