ithdrew' THEN 'Withdrew'&#xA;&#x9;&#x9;WHEN 'Business Case Candidate Rejected' THEN 'Not Selected'&#xA;&#x9;&#x9;ELSE CSM.NewReferralStatusDesc&#xA;&#x9;END&#xA;&#x9;ELSE CSM.AbacusReferralStatusDesc&#xA;&#x9;END) &#xA;    ,[OriginalAppStatusDesc] = CS.[ATASEmployeeStatusDes]&#xA;    ,[RejectionReasonDesc] = CS.[RejectionReasonDesc]&#xA;    ,[AppliedDt] = CS.[AppliedDt]&#xA;    ,[LastModifiedDttm] = CS.[LastModifiedDttm]&#xA;    ,[CreateUserId] = CS.[CreateUserId]&#xA;    ,[CreateDttm] = CS.[CreateDttm]&#xA;    ,[UpdateUserId] = CS.[UpdateUserId]&#xA;    ,[UpdateDttm] = CS.[UpdateDttm]&#xA;FROM (SELECT * &#xA;FROM [EmpRef].[ApplicantStatus]  WITH(NOLOCK)&#xA;WHERE RequisitionId = @Requisition_Id&#xA;AND RequisitionSourceCd = @Requisition_source_cd&#xA;AND EmailId = @EmailId) CS&#xA;INNER JOIN [EmpRef].[CandidateStatusMapping] CSM WITH(NOLOCK)&#xA;ON CS.ATASEmployeeStatusDes = CSM.ATASReferralStatusDesc  &#xA;INNER JOIN [EmpRef].[CodeDetail] CD WITH(NOLOCK)&#xA;ON CS.[RequisitionSourceCd] = CD.[CodeTxt]&#xA;AND CD.[CategoryNbr] = 2&#xA;INNER JOIN ##SearchCorrectPeopleKey pp&#xA;ON CS.EmailId = PP.EmailStage&#xA;INNER JOIN [2761_EnterpriseODS].[EmpRef].[ApplicantStatus] CS_ODS WITH(NOLOCK)&#xA;ON CS.[RequisitionId] = CS_ODS.[RequisitionId]&#xA;AND CS.[RequisitionSourceCd] = CS_ODS.[RequisitionSourceCd]&#xA;AND PP.PeopleKey = CS_ODS.[PeopleKey]&#xA;AND CS.[LastModifiedDttm] &gt; CS_ODS.[LastModifiedDttm]&#xA;&#xA;END TRY&#xA;BEGIN CATCH&#xA;&#xA;&#x9;INSERT INTO ##ErrorTable ([CandidateSourceCd], [TableName], [ErrorDesc])&#xA;&#x9;SELECT @Candidate_source_cd, 'ApplicantStatus', 'SSIS_ER_ERTblUpdates. Update ApplicantStatus Failed; requisitonId: ' + @Requisition_Id + '; RequisitonSourceCd: ' + @Requisition_source_cd + '; EmailId: ' + @EmailId + '; DescError: ' + ERROR_MESSAGE()&#xA;&#xA;END CATCH&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
