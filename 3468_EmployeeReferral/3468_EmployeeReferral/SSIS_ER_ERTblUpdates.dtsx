                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT RF.[EmailId], CASE  WHEN RF_ODS.[PeopleKey] IS NULL THEN 'Insert' ELSE 'Update' END as TypeRecord
FROM  [EmpRef].[Referrer] RF WITH(NOLOCK)
INNER JOIN [2761_EnterpriseODS].[MRDRSensitive].[People] PP WITH(NOLOCK)
ON substring(RF.EmailId,1, charindex('@',RF.EmailId) - 1) = pp.EnterpriseId
LEFT JOIN [2761_EnterpriseODS].[EmpRef].[Referrer] RF_ODS WITH(NOLOCK)
ON PP.PeopleKey = RF_ODS.PeopleKey
WHERE (RF_ODS.[PeopleKey] IS NULL
	AND PP.[PeopleKey] NOT IN (SELECT [PeopleKey] FROM [2761_EnterpriseODS].[EmpRef].Referrer WITH(NOLOCK)))
	OR (PP.[PeopleKey] = RF_ODS.[PeopleKey]	  
