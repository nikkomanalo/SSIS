                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Select EmpRef.CandidateStatus.RequisitionId
                                            ,EmpRef.CandidateStatus.RequisitionSourceCd
                                            ,EmpRef.CandidateStatus.CandidateId
                                            ,EmpRef.CandidateStatus.CandidateSourceCd
                                            ,EmpRef.CandidateStatus.PeopleKey
                                            ,EmpRef.CandidateStatus.SystemSourceDesc
                                            ,ReferralStatusDesc
                                            ,OriginalReferStatusDesc
                                            ,ReferredDt
                                            ,HiredDt
                                            ,EmpRef.CandidateStatus.LastModifiedDttm
                                            ,EmpRef.CandidateStatus.CreateUserId
                                            ,EmpRef.CandidateStatus.CreateDttm
                                            ,EmpRef.CandidateStatus.UpdateUserId
                                            ,EmpRef.CandidateStatus.UpdateDttm 
                                            ,EmpRef.CandidateStatus.CSWLatestStep
                                            ,EmpRef.CandidateStatus.CSWLatestDate
											,EmpRef.CandidateStatus.ApplicationRequisitionTitle
                                            from EmpRef.CandidateStatus left Join EmpRef.Requisition
                                            On EmpRef.CandidateStatus.RequisitionId = EmpRef.Requisition.RequisitionId 
                                            and EmpRef.CandidateStatus.RequisitionSourceCd =
                                            EmpRef.Requisition.RequisitionSourceCd 
                                            Inner Join EmpRef.Candidate 
                                            On EmpRef.CandidateStatus.CandidateId = EmpRef.Candidate.CandidateId 
                                            and EmpRef.CandidateStatus.CandidateSourceCd = EmpRef.Candidate.CandidateSourceCd 
                                            Inner Join EmpRef.Referrer 
                                            On EmpRef.CandidateStatus.PeopleKey = EmpRef.Referrer.PeopleKey 
                                            Where (EmpRef.Requisition.RequisitionSourceCd NOT IN ('CWS-9002') OR ISNULL(EmpRef.Requisition.RequisitionSourceCd,'') = '')
                                            AND EmpRef.CandidateStatus.CreateDttm IS NOT NULL 
                                            AND EmpRef.CandidateStatus.UpdateDttm&gt;?
                                            ORDER BY EmpRef.CandidateStatus.UpdateDttm ASC</property>
