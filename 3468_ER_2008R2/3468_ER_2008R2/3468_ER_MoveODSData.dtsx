                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT RQ.RequisitionId, RQ.RequisitionSourceCd, CASE  WHEN RQ_ODS.[RequisitionId] IS NULL THEN 'Insert' ELSE 'Update' END as TypeRecord
FROM [EmpRef].[Requisition] RQ WITH(NOLOCK)
LEFT JOIN ##ValidateURLDate VL
	ON RQ.RequisitionId = VL.RequisitionId
