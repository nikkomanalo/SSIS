              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{BC0E8781-596B-45A2-A8F7-E2CD15C02DC1}"
                  SQLTask:SqlStatementSource="DECLARE @Requisition_Id VARCHAR(20), &#xA;&#x9;&#x9;@Requisition_source_cd VARCHAR(20), &#xA;&#x9;&#x9;@Candidate_Id VARCHAR(20),&#xA;&#x9;&#x9;@Candidate_source_cd VARCHAR(20), &#xA;&#x9;&#x9;@EmailId varchar(100)&#xA;&#xA;Set @Requisition_Id = ?&#xA;Set @Requisition_source_cd = ?&#xA;Set @Candidate_Id = ?&#xA;Set @Candidate_source_cd = ?&#xA;Set @EmailId = ?&#xA;&#xA;&#xA;BEGIN TRY&#xA;&#xA;INSERT [2761_EnterpriseODS_Rel1].[EmpRef].[CandidateStatus]&#xA;&#x9;([RequisitionId]&#xA;&#x9;,[RequisitionSourceCd]&#xA;&#x9;,[CandidateId]&#xA;&#x9;,[CandidateSourceCd]&#xA;&#x9;,[PeopleKey]&#xA;&#x9;,[SystemSourceDesc]&#xA;&#x9;,[ReferralStatusDesc]&#xA;&#x9;,[OriginalReferStatusDesc]&#xA;&#x9;,[ReferredDt]&#xA;&#x9;,[HiredDt]&#xA;&#x9;,[LastModifiedDttm]&#xA;&#x9;,[CreateUserId]&#xA;&#x9;,[CreateDttm]&#xA;&#x9;,[UpdateUserId]&#xA;&#x9;,[UpdateDttm]&#xA;&#x9;--ER R9 RBE 10/03/2016 START&#xA;&#x9;,[CSWLatestStep]&#xA;&#x9;,[CSWLatestDate]&#xA;&#x9;--ER R9 RBE 10/03/2016 End&#xA;&#x9;--ER R10 RBE 03/01/2017 START&#xA;&#x9;,[ApplicationRequisitionTitle])&#xA;&#x9;--ER R10 RBE 03/01/2017 End&#xA;SELECT CS.[RequisitionId]&#xA;&#x9;&#x9;,CS.[RequisitionSourceCd]&#xA;&#x9;&#x9;,CS.[CandidateId]&#xA;&#x9;&#x9;,CS.[CandidateSourceCd]&#xA;&#x9;&#x9;,PP.[PeopleKey] AS PeopleKey&#xA;&#x9;&#x9;,CD.DecodeTxt as SystemSourceDesc&#xA;&#x9;&#x9;,CONVERT(VARCHAR(50)&#xA;&#x9;&#x9;,CASE CS.[CandidateSourceCd]&#xA;&#x9;&#x9;&#x9;WHEN 'ATAS1' THEN CASE CS.[ATASReferralStatusDesc]&#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'Candidate Reject - Not Applicant' THEN &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CASE CS.[RejectionReasonDesc]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Moved to Hiring Requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: More Qualified for other position in our organization' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive reject: Matched to new Fiscal Year requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--ER R6 RBE 07/21/2015 START&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Moved to Candidate Management or Hiring Requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--ER R6 RBE 07/21/2015 END&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 'Candidate Rejected'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'Candidate Reject — No Business Need' THEN &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CASE CS.[RejectionReasonDesc]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Moved to Hiring Requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: More Qualified for other position in our organization' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive reject: Matched to new Fiscal Year requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--ER R6 RBE 07/21/2015 START&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Moved to Candidate Management or Hiring Requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--ER R6 RBE 07/21/2015 END&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 'Candidate Rejected'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END  &#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'Candidate Rejected' THEN &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CASE CS.[RejectionReasonDesc]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Moved to Hiring Requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: More Qualified for other position in our organization' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive reject: Matched to new Fiscal Year requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--ER R6 RBE 07/21/2015 START&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Positive Reject: Moved to Candidate Management or Hiring Requisition' THEN 'Moved to other Job'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--ER R6 RBE 07/21/2015 END&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 'Candidate Rejected'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END  &#xA;&#x9;&#x9;&#x9;&#x9;--WHEN 'Candidate Withdrew' THEN 'Applicant Withdrew'&#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'Business Case Candidate Rejected' THEN 'Applicant Not Selected'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE CSM.NewReferralStatusDesc&#xA;&#x9;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;ELSE CSM.[AbacusReferralStatusDesc]&#xA;&#x9;&#x9;END) AS ReferralStatusDesc&#xA;&#x9;&#x9;,CS.ATASReferralStatusDesc&#xA;&#x9;&#x9;,CS.[ReferredDt]&#xA;&#x9;&#x9;,CS.[HiredDt]&#xA;&#x9;&#x9;,CS.[LastModifiedDttm]&#xA;&#x9;&#x9;,CS.[CreateUserId]&#xA;&#x9;&#x9;,CS.[CreateDttm]&#xA;&#x9;&#x9;,CS.[UpdateUserId]&#xA;&#x9;&#x9;,CS.[UpdateDttm]&#xA;&#x9;&#x9;--ER R9 RBE 10/03/2016 START&#xA;&#x9;&#x9;,CS.[CSWLatestStep]&#xA;&#x9;&#x9;,CS.[CSWLatestDate]&#xA;&#x9;&#x9;--ER R9 RBE 10/03/2016 END&#xA;&#x9;&#x9;--ER R10 RBE 03/01/2017 START&#xA;&#x9;&#x9;,CS.[ApplicationRequisitionTitle]&#xA;&#x9;&#x9;--ER R10 RBE 03/01/2017 START&#xA;FROM [EmpRef].[CandidateStatus] CS WITH(NOLOCK)&#x9;&#xA;LEFT JOIN [EmpRef].[CandidateStatusMapping] CSM WITH(NOLOCK)&#xA;ON CS.[ATASReferralStatusDesc] = CSM.[ATASReferralStatusDesc]&#xA;INNER JOIN [EmpRef].[CodeDetail] CD WITH(NOLOCK)&#xA;ON CS.[RequisitionSourceCd] = CD.[CodeTxt]&#xA;AND CD.[CategoryNbr] = 2&#xA;INNER JOIN ##SearchCorrectPeopleKey PP&#x9; &#xA;ON CS.EmailId = PP.EmailStage&#xA;LEFT JOIN [2761_EnterpriseODS_Rel1].[EmpRef].[CandidateStatus] CS_ODS WITH(NOLOCK)&#xA;ON (CS.[RequisitionId] = CS_ODS.[RequisitionId]&#xA;&#x9;&#x9;AND CS.[RequisitionSourceCd] = CS_ODS.[RequisitionSourceCd]&#xA;&#x9;&#x9;AND CS.[CandidateId] = CS_ODS.[CandidateId]&#xA;&#x9;&#x9;AND CS.[CandidateSourceCd] = CS_ODS.[CandidateSourceCd]&#xA;&#x9;&#x9;AND PP.PeopleKey = CS_ODS.[PeopleKey])&#xA;&#x9;&#x9;&#xA;/***CSC 080814 CandStat Fix: Start***/&#xA;/***Removed OR left join, logic will apply to both ATAS and ABACUS, plus current join for ABACUS only looks at four keys***/&#xA;&#xA;--&#x9;&#x9;AND CS.RequisitionSourceCd NOT IN (SELECT CodeTxt FROM Empref.CodeDetail WHERE CategoryNbr = 2 AND CodeTxt LIKE 'AB-%'))&#xA;--OR (CS.[RequisitionSourceCd] = CS_ODS.[RequisitionSourceCd]&#xA;--&#x9;&#x9;AND CS.[CandidateId] = CS_ODS.[CandidateId]&#xA;--&#x9;&#x9;AND CS.[CandidateSourceCd] = CS_ODS.[CandidateSourceCd]&#xA;--&#x9;&#x9;AND PP.PeopleKey = CS_ODS.[PeopleKey]&#xA;--&#x9;&#x9;AND CS.RequisitionSourceCd IN (SELECT CodeTxt FROM Empref.CodeDetail WHERE CategoryNbr = 2 AND CodeTxt LIKE 'AB-%'))&#xA;&#xA;/***Removed OR left join, logic will apply to both ATAS and ABACUS, plus current join for ABACUS only looks at four keys***/&#xA;/***CSC 080814 CandStat Fix: End***/&#xA;&#xA;WHERE CS_ODS.[RequisitionId] IS NULL&#xA;&#x9;&#x9;AND CS_ODS.[RequisitionSourceCd] IS NULL&#xA;&#x9;&#x9;AND CS.RequisitionId = @Requisition_Id&#xA;&#x9;&#x9;AND CS.RequisitionSourceCd = @Requisition_source_cd&#xA;&#x9;&#x9;AND CS.CandidateId = @Candidate_Id&#xA;&#x9;&#x9;AND CS.CandidateSourceCd = @Candidate_source_cd&#xA;&#x9;&#x9;AND CS.EmailId = @EmailId&#xA;/***CSC 081314 CandStat Fix: Added CandidateID verification on the final ODS table***/&#xA;&#x9;&#x9;AND CS_ODS.[CandidateId] IS NULL&#x9;&#x9;&#xA;END TRY&#xA;BEGIN CATCH&#xA;&#xA;&#x9;INSERT INTO ##ErrorTable ([CandidateSourceCd], [TableName], [ErrorDesc])&#xA;&#x9;SELECT @Candidate_source_cd, 'CandidateStatus', 'SSIS_ER_ERTblUpdates. Insert CandidateStatus Failed; requisitonId: ' + @Requisition_Id + '; RequisitonSourceCd: ' + @Requisition_source_cd + '; CandidateId: ' + @Candidate_Id + '; CandidateSourceCd: ' + @Candidate_source_cd + '; EmailId: ' + @EmailId + '; DescError: ' + ERROR_MESSAGE()&#xA;&#xA;END CATCH" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
