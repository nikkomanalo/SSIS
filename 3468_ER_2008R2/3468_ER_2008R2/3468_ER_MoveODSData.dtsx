                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT RF.[EmailId], CASE  WHEN RF_ODS.[PeopleKey] IS NULL THEN 'Insert' ELSE 'Update' END as TypeRecord
FROM  [EmpRef].[Referrer] RF WITH(NOLOCK)
INNER JOIN [2761_EnterpriseODS_Rel1].[MRDRSensitive].[People] PP WITH(NOLOCK)
ON substring(RF.EmailId,1, charindex('@',RF.EmailId) - 1) = pp.EnterpriseId
LEFT JOIN [2761_EnterpriseODS_Rel1].[EmpRef].[Referrer] RF_ODS WITH(NOLOCK)
ON PP.PeopleKey = RF_ODS.PeopleKey
WHERE (RF_ODS.[PeopleKey] IS NULL
	AND PP.[PeopleKey] NOT IN (SELECT [PeopleKey] FROM [2761_EnterpriseODS_Rel1].[EmpRef].Referrer WITH(NOLOCK)))
	OR (PP.[PeopleKey] = RF_ODS.[PeopleKey]	  
		AND NOT (
			RF_ODS.[EnterpriseId] = PP.[EnterpriseId] 
			AND RF_ODS.[EmployeeId] = RF.[EmployeeId] 
			AND RF_ODS.[FirstNm] = PP.[FirstNm]
			AND RF_ODS.[LastNm] = PP.[LastNm]
