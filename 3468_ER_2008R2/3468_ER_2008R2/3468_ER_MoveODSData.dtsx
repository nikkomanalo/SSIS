                  SQLTask:SqlStatementSource="DECLARE @requisition_id NVARCHAR(20),&#xA;&#x9;&#x9;@system_source_cd NVARCHAR(20),&#xA;&#x9;&#x9;@LocationDesc varchar(8000),&#xA;&#x9;&#x9;@IntJobDescURL varchar(255),&#xA;&#x9;&#x9;@IntReferURL varchar(255),&#xA;&#x9;&#x9;@ExtURL varchar(255),&#xA;&#x9;&#x9;@IntJobDescURLAba varchar(255),&#xA;&#x9;&#x9;@IntReferURLAba varchar(255),&#xA;&#x9;&#x9;@ExtURLAbaInd varchar(255),&#xA;&#x9;&#x9;@ExtURLAbaPh varchar(255),&#xA;&#x9;&#x9;@IntJobDescOIPURL varchar(255),&#xA;&#x9;&#x9;@CandidateSourceCd NVARCHAR(20),&#xA;&#x9;&#x9;@IntReferExperiumURL varchar(255)&#xA;&#xA;Set @requisition_id = ?&#xA;Set @system_source_cd = ?&#xA;Set @LocationDesc = ?&#xA;Set @IntJobDescURL = ?&#xA;Set @IntReferURL = ?&#xA;Set @ExtURL = ?&#xA;Set @IntJobDescURLAba = ?&#xA;Set @IntReferURLAba = ?&#xA;Set @ExtURLAbaInd = ?&#xA;Set @ExtURLAbaPh = ?&#xA;Set @IntJobDescOIPURL = ?&#xA;Set @CandidateSourceCd = ?&#xA;Set @IntReferExperiumURL = ?&#xA;&#xA;&#xA;BEGIN TRY&#xA;&#xA;UPDATE [2761_EnterpriseODS_Rel1].[EmpRef].[Requisition]&#xA;SET &#x9; [RequisitionId] = RQ.[RequisitionId],&#xA;&#x9;&#x9; [RequisitionSourceCd] = RQ.[RequisitionSourceCd],&#xA;&#x9;&#x9; [SystemSourceDesc] = CD.[DecodeTxt],&#xA;&#x9;&#x9; --ER R5: RE 05.19.2015 - REPLACE long Hypen with a dash START&#xA;&#x9;&#x9; [RequisitionTitle] = REPLACE(RQ.[RequisitionTitle], N' ' + NCHAR(8211) + N' ',N'-'),&#xA;&#x9;&#x9; --ER R5: RE 05.19.2015 - REPLACE long Hypen with a dash START&#xA;&#x9;&#x9; [RequisitionType] = RQ.[RequisitionType],&#xA;&#x9;&#x9; [CountryCd] = RQ.[CountryCd],&#xA;&#x9;&#x9; [CountryDesc] = RQ.[CountryDesc],&#xA;&#x9;&#x9; [LevelCd] = RQ.[LevelCd], &#xA;&#x9;&#x9; [LevelDesc] = RQ.[LevelDesc],&#xA;&#x9;&#x9; [WorkForceCd] = RQ.[WorkForceCd],&#xA;&#x9;&#x9; [WorkForceDesc] = RQ.[WorkForceDesc],&#xA;&#x9;&#x9; [WorkGroupCd] = RQ.[WorkGroupCd],&#xA;&#x9;&#x9; [WorkGroupDesc] = RQ.[WorkGroupDesc],&#xA;&#x9;&#x9; [SpecializationCd] = RQ.[SpecializationCd],&#xA;&#x9;&#x9; [SpecializationDesc] = RQ.[SpecializationDesc],&#xA;&#x9;&#x9; [CityCd] = RQ.[CityCd],&#xA;&#x9;&#x9; [CityDesc] = RQ.[CityDesc],&#xA;&#x9;&#x9; --ER R5: RE 05.19.2015 - REPLACE long Hypen with a dash START&#xA;&#x9;&#x9; [Job1Desc] = REPLACE(RQ.[Job1Desc], N' ' + NCHAR(8211) + N' ',N'-'),&#xA;&#x9;&#x9; [Job2Desc] = REPLACE(RQ.[Job2Desc], N' ' + NCHAR(8211) + N' ',N'-'),&#xA;&#x9;&#x9;--ER R5: RE 05.19.2015 - REPLACE long Hypen with a dash START&#xA;&#x9;&#x9; [Qualification1Desc] = RQ.[Qualification1Desc],&#xA;&#x9;&#x9; [Qualification2Desc] = RQ.[Qualification2Desc], &#xA;&#x9;&#x9; [LocationDesc] = CASE CD.[DecodeTxt]&#xA;&#x9;&#x9;&#x9;WHEN 'ATAS' THEN CONVERT(VARCHAR(1500),@LocationDesc)&#xA;&#x9;&#x9;&#x9;ELSE ''&#xA;&#x9;   &#x9; END,&#xA;&#x9;&#x9; [RequisitionStatusDesc] = RQ.[RequisitionStatusDesc],&#xA;&#x9;&#x9; [RequisitionStatusDetailDesc] = RQ.[RequisitionStatusDetailDesc], &#xA;&#x9;&#x9; [SourcingRequestStatusDesc] = RQ.[SourcingRequestStatusDesc], &#xA;&#x9;&#x9; [InternalJobDescURL] = CASE CD.[CodeTxt]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'JB-21063' THEN REPLACE(@IntJobDescURL,'&lt;JobId&gt;',RQ.[RequisitionId])&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'CWS-9002' THEN REPLACE(@IntJobDescOIPURL,'&lt;JobId&gt;',RQ.[RequisitionId])&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE REPLACE(REPLACE(REPLACE(@IntJobDescURLAba,'&lt;jobcode&gt;',RQ.[RequisitionId]),'&lt;#&gt;',SUBSTRING(RQ.[InternalJobDescURL],1,CHARINDEX(';',RQ.[InternalJobDescURL])-1)),'&lt;Countrycode&gt;',SUBSTRING(RQ.[InternalJobDescURL],CHARINDEX(';',RQ.[InternalJobDescURL])+1,LEN(RQ.[InternalJobDescURL])))&#xA;&#x9;&#x9;&#x9;&#x9;END, &#xA;&#x9;&#x9; [InternalReferURL] = CASE CD.[CodeTxt]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'JB-21063' THEN&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE RQ.CountryCd&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'RU' THEN REPLACE(REPLACE(@IntReferExperiumURL,'&lt;JobId&gt;',RQ.[RequisitionId]),'&lt;CountryCd&gt;',RQ.CountryCd)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'KZ' THEN REPLACE(REPLACE(@IntReferExperiumURL,'&lt;JobId&gt;',RQ.[RequisitionId]),'&lt;CountryCd&gt;',RQ.CountryCd)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE REPLACE(@IntReferURL,'&lt;JobId&gt;',RQ.[RequisitionId])&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'CWS-9002' THEN ''&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE REPLACE(REPLACE(REPLACE(@IntReferURLAba,'&lt;jobcode&gt;',RQ.[RequisitionId]),'&lt;#&gt;',SUBSTRING(RQ.[InternalJobDescURL],1,CHARINDEX(';',RQ.[InternalJobDescURL])-1)),'&lt;Countrycode&gt;',SUBSTRING(RQ.[InternalJobDescURL],CHARINDEX(';',RQ.[InternalJobDescURL])+1,LEN(RQ.[InternalJobDescURL])))&#xA;&#x9;&#x9;&#x9;&#x9;END,&#xA;&#x9;&#x9; [ExternalURL] = CASE CD.[CodeTxt]&#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'JB-21063' THEN REPLACE(@ExtURL,'&lt;JobId&gt;',RQ.[RequisitionId])&#xA;&#x9;&#x9;&#x9;&#x9;WHEN 'CWS-9002' THEN ''&#xA;&#x9;&#x9;&#x9;&#x9;ELSE CASE SUBSTRING(RQ.[InternalJobDescURL],CHARINDEX(';',RQ.[InternalJobDescURL])+1,LEN(RQ.[InternalJobDescURL]))&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Ind' THEN REPLACE(REPLACE(@ExtURLAbaInd,'&lt;jobcode&gt;',RQ.[RequisitionId]),'&lt;#&gt;',SUBSTRING(RQ.[InternalJobDescURL],1,CHARINDEX(';',RQ.[InternalJobDescURL])-1))&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Phi' THEN REPLACE(REPLACE(@ExtURLAbaPh,'&lt;jobcode&gt;',RQ.[RequisitionId]),'&lt;#&gt;',SUBSTRING(RQ.[InternalJobDescURL],1,CHARINDEX(';',RQ.[InternalJobDescURL])-1))&#xA;&#x9;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;END,&#xA;&#x9;&#x9; [LastModifiedDttm] = RQ.[LastModifiedDttm],&#xA;&#x9;&#x9; [RecruiterNm] = CRN.[RecruiterNmFinal],&#xA;&#x9;&#x9; [CareerTrackCd] = RQ.[CareerTrackCd],&#xA;&#x9;     [CareerTrackDesc] = RQ.[CareerTrackDesc],&#xA;&#x9;     [JobFamilyGroupCd]  = RQ.[JobFamilyGroupCd],&#xA;&#x9;     [JobFamilyGroupDesc]  = RQ.[JobFamilyGroupDesc],&#xA;&#x9;     [JobCd] = RQ.[JobCd],&#xA;&#x9;     [JobDesc] = RQ.[JobDesc],&#xA;&#x9;     [ContractBasedCd] = CASE RQ.[ContractBasedCd] &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN 'Yes' THEN 'Yes'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 'No'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; END,&#xA;&#x9;     [Travel] = RQ.[Travel],&#xA;&#x9;     [RequisitionDTECd] = RQ.[RequisitionDTECd],&#xA;&#x9;     [AdditionalSpecializationCd] = RQ.[AdditionalSpecializationCd],&#xA;&#x9;     [AdditionalSpecializationDesc]= RQ.[AdditionalSpecializationDesc],&#xA;&#x9;     [CreateDttm] = RQ.[CreateDttm],&#xA;&#x9;     [UpdateUserId] = RQ.[UpdateUserId], &#xA;&#x9;     [UpdateDttm] = RQ.[UpdateDttm],&#xA;&#x9;     --ER R4: CC 02.10.2015 - Add new Field: InternalOnly - Start&#xA;&#x9;     [InternalOnly]= RQ.[InternalOnly],&#xA;&#x9;     --ER R4: CC 02.10.2015 - Add new Field: InternalOnly - End&#xA;&#x9;    --ER R5: RE 05.06.2015 - Add new Field: CompanyCd - START&#xA;&#x9;     [CompanyCd]= RQ.[CompanyCd],&#xA;&#x9;    --ER R5: RE 05.06.2015 - Add new Field: CompanyCd - END&#xA;&#x9;    --ER R8: RBE 03.23.2016 - Add new Field: DTEOrgUnit Values- START&#xA;&#x9;      [DTEOrgUnit1] = RQ.[DTEOrgUnit1],&#xA;&#x9;      [DTEOrgUnit2] = RQ.[DTEOrgUnit2], &#xA;&#x9;    --ER R8: RBE 03.23.2016 - Add new Field: DTEOrgUnit Values-  - END&#xA;                        [OpenDttm] = RQ.[OpenDttm],&#xA;&#x9;      [DTEOrgUnit3] = RQ.[DTEOrgUnit3],&#xA;&#x9;      [DTEOrgUnit4] = RQ.[DTEOrgUnit4], &#xA;&#x9;      [DTEOrgUnit5] = RQ.[DTEOrgUnit5],&#xA;&#x9;      [DTEOrgUnit6] = RQ.[DTEOrgUnit6]&#xA;FROM [EmpRef].CodeDetail CD WITH(NOLOCK)&#xA;&#x9; ,(SELECT * FROM [EmpRef].[Requisition] SQ WITH(NOLOCK)&#xA;&#x9;   WHERE SQ.[RequisitionId] = @requisition_id AND SQ.[RequisitionSourceCd] = @system_source_cd) RQ&#xA;INNER JOIN ##CalculateRecruiterNm CRN&#xA;ON CRN.RequisitionId = RQ.RequisitionId&#xA;AND CRN.RequisitionSourceCd = RQ.RequisitionSourceCd&#xA;LEFT JOIN [2761_EnterpriseODS_Rel1].[MRDRSensitive].[People] pp  WITH(NOLOCK)&#xA;ON substring(RQ.[RecruiterNm],charindex(':',RQ.[RecruiterNm])+1, charindex('@',RQ.[RecruiterNm]) - 1-charindex(':',RQ.[RecruiterNm])) = pp.EnterpriseId&#xA;INNER JOIN [2761_EnterpriseODS_Rel1].[EmpRef].[Requisition] RQ_ODS WITH(NOLOCK)&#xA;ON RQ.[RequisitionId]= RQ_ODS.[RequisitionId]&#xA;&#x9;  AND RQ.[RequisitionSourceCd] = RQ_ODS.[RequisitionSourceCd]&#xA;&#x9;  AND RQ.[LastModifiedDttm] &gt; RQ_ODS.[LastModifiedDttm]&#xA;WHERE RQ.RequisitionSourceCd = CD.CodeTxt&#xA;&#x9;  AND CD.CategoryNbr = 2&#xA;&#xA;END TRY&#xA;BEGIN CATCH&#xA;&#xA;&#x9;INSERT INTO ##ErrorTable ([CandidateSourceCd], [TableName], [ErrorDesc])&#xA;&#x9;SELECT @CandidateSourceCd, 'Requisition', 'SSIS_ER_ERTblUpdates. Update requisition Failed; requisitonId: ' + @requisition_id + '; RequisitonSourceCd: ' + @system_source_cd + '; DescError: ' + ERROR_MESSAGE()&#xA;&#xA;END CATCH" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
