          DTS:ObjectName="Execute SQL Task"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{BC0E8781-596B-45A2-A8F7-E2CD15C02DC1}"
              SQLTask:SqlStatementSource="DECLARE @CandidateSourceCd as VARCHAR(20)&#xA;&#xA;Set @CandidateSourceCd = ?&#xA;&#xA;BEGIN TRY&#xA;&#xA;INSERT INTO ##SearchCorrectPeopleKey([EmailStage], [FinalEmail], [PeopleKey]) &#xA;SELECT distinct CS.EmailId, PP.InternalMailAddressTxt, pp.PeopleKey&#xA;FROM [EmpRef].[CandidateStatus] CS WITH(NOLOCK)&#xA;INNER JOIN [EmpRef].[CodeDetail] CD WITH(NOLOCK)&#xA;ON CS.[RequisitionSourceCd] = CD.[CodeTxt]&#xA;AND CD.[CategoryNbr] = 2&#xA;INNER JOIN[2761_EnterpriseODS_Rel1].[MRDRSensitive].[People] PP WITH(NOLOCK)&#xA;ON substring(CS.EmailId ,1, charindex('@',CS.EmailId) - 1) = PP.EnterpriseId&#xA;&#xA;END TRY&#xA;BEGIN CATCH&#xA;&#xA;&#x9;INSERT INTO ##ErrorTable ([CandidateSourceCd], [TableName], [ErrorDesc])&#xA;&#x9;SELECT @CandidateSourceCd, 'CandidateStatus', 'SSIS_ER_ERTblUpdates. Search Correct Mail Failed' + '; DescError: ' + ERROR_MESSAGE()&#xA;&#xA;&#xA;END CATCH&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
              <SQLTask:ParameterBinding
