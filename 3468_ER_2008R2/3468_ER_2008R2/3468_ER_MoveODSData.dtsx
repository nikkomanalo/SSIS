              SQLTask:SqlStatementSource="--initially temporary tables are eliminated in case there&#xA;IF object_id('tempdb..##SearchCorrectPeopleKey') IS NOT NULL &#xA;BEGIN &#xA; DROP TABLE ##SearchCorrectPeopleKey&#xA;END &#xA;GO &#xA;&#xA;--Create the temp table for Requisition TransformProcess&#xA;CREATE TABLE ##SearchCorrectPeopleKey(&#xA; [EmailStage] [varchar](100) NULL, &#xA; [FinalEmail] [varchar](100) NULL, &#xA; [PeopleKey] [int] NULL&#xA;) &#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
