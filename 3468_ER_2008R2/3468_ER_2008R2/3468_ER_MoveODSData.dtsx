              SQLTask:SqlStatementSource="&#xA;DECLARE @CandidateSourceCd NVARCHAR(20)&#xA;&#xA;Set @CandidateSourceCd = ?&#xA;&#xA;&#xA;BEGIN TRY&#xA;&#xA;Insert into ##CalculateRecruiterNm ([RecruiterNmFinal], [RequisitionId], [RequisitionSourceCd])&#xA;SELECT&#x9;CASE WHEN pp.InternalMailAddressTxt is NULL THEN RQ.[RecruiterNm]&#xA;&#x9;&#x9;&#x9; WHEN pp.InternalMailAddressTxt NOT LIKE '%accenture%' THEN RQ.[RecruiterNm]&#xA;&#x9;&#x9;&#x9; ELSE substring(RQ.[RecruiterNm],1, charindex(':',RQ.[RecruiterNm])) + pp.InternalMailAddressTxt&#xA;&#x9;&#x9;&#x9; END as RecruiterNmFinal,&#xA;&#x9;&#x9;RQ.RequisitionId, &#xA;&#x9;&#x9;RQ.RequisitionSourceCd&#xA;FROM [EmpRef].[Requisition] RQ WITH(NOLOCK)&#xA;LEFT JOIN [2761_EnterpriseODS_Rel1].[EmpRef].[Requisition] RQ_ODS WITH(NOLOCK)&#xA;&#x9;ON RQ.[RequisitionId]= RQ_ODS.[RequisitionId]&#xA;&#x9;AND RQ.[RequisitionSourceCd] = RQ_ODS.[RequisitionSourceCd]&#xA;LEFT JOIN [2761_EnterpriseODS_Rel1].[MRDRSensitive].[People] pp WITH(NOLOCK)&#xA;ON substring(RQ.[RecruiterNm],charindex(':',RQ.[RecruiterNm])+1, charindex('@',RQ.[RecruiterNm]) - 1-charindex(':',RQ.[RecruiterNm])) = pp.EnterpriseId&#xA;WHERE ((RQ.[RequisitionSourceCd] = RQ_ODS.[RequisitionSourceCd]&#xA;&#x9;AND RQ.[LastModifiedDttm] &gt; RQ_ODS.[LastModifiedDttm])&#xA;&#x9;OR (RQ_ODS.[RequisitionId] is null&#xA;&#x9;AND RQ_ODS.[RequisitionSourceCd] is null))&#xA;GROUP BY RQ.RequisitionId, RQ.RequisitionSourceCd, RQ.RecruiterNm, pp.InternalMailAddressTxt&#xA;&#xA;END TRY&#xA;BEGIN CATCH&#xA;&#xA;&#x9;INSERT INTO ##ErrorTable ([CandidateSourceCd], [TableName], [ErrorDesc])&#xA;&#x9;SELECT @CandidateSourceCd, 'Requisition', 'SSIS_ER_ERTblUpdates. Calculate RecruiterMail Failed; DescError: ' + ERROR_MESSAGE()&#xA;&#xA;END CATCH" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
