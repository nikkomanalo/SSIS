--CASE  WHEN CS_ODS.CandidateId IS NULL THEN 'Insert' ELSE 'Update' END as TypeRecord
CASE  WHEN CS.RequisitionId='99999999' AND CS.RequisitionSourceCd LIKE 'AB-%'
THEN 
	CASE  WHEN CS.CandidateId IN (
		SELECT CS_ODS1.CandidateId from 
		[2761_EnterpriseODS_Rel1].[EmpRef].[CandidateStatus] CS_ODS1, 
		[2761_EnterpriseODS_Stage_Rel1].[EmpRef].[CandidateStatus] CS1
		WHERE (CS1.[RequisitionSourceCd] = CS_ODS1.[RequisitionSourceCd]
			AND CS1.[CandidateId] = CS_ODS1.[CandidateId]
			AND PP.[PeopleKey] = CS_ODS1.[PeopleKey]
			AND CS1.[LastModifiedDttm] &gt; CS_ODS1.[LastModifiedDttm]
			AND CS_ODS1.[CandidateSourceCd] = CS1.[CandidateSourceCd])
		 )
	THEN 
		'Update'
	ELSE 
		'Insert'
	END	
ELSE 
	CASE  WHEN CS_ODS.CandidateId IS NULL
	THEN 
		'Insert'		
	ELSE 
		'Update'
	END	
END as TypeRecord

