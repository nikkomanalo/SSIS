        <DTS:Executable
          DTS:refId="Package\Candidate Status\Get People Data"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{66882fc7-7193-44f9-a691-2d81887229a0}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="Get People Data"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{BC0E8781-596B-45A2-A8F7-E2CD15C02DC1}"
              SQLTask:SqlStatementSource="CREATE TABLE #varCandidate&#xA;(&#xA;&#x9;CandidateID varchar(20),&#xA;&#x9;CandidateSourceCd varchar(20),&#xA;&#x9;HiredDt datetime,&#xA;&#x9;PeopleKey int null,&#xA;&#x9;EnterpriseID varchar(50) null,&#xA;&#x9;PersonnelNbr varchar(10) null,&#xA;&#x9;CandidateIDDummy varchar(100),&#xA;&#x9;firstnm varchar(200) null,&#xA;&#x9;lastnm varchar(200) null&#xA;)&#xA;&#xA;&#xA;CREATE INDEX idx_CandidateID&#xA;ON #varCandidate (CandidateID);&#xA;&#xA;--temp table to hold people data w/ updates&#xA;CREATE TABLE #varPeople&#xA;(&#xA;&#x9;CandidateID varchar(50),&#xA;&#x9;ActualCandidateID varchar(50),&#xA;&#x9;PeopleKey int null,&#xA;&#x9;EnterpriseID varchar(50) null,&#xA;&#x9;PersonnelNbr varchar(10) null,&#xA;&#x9;CandidateSourceCd varchar(20) null,&#xA;&#x9;FirstNm varchar(200) null,&#xA;&#x9;LastNm varchar(200) null&#xA;)&#xA;&#xA;CREATE INDEX idx_CandidateID&#xA;ON #varPeople (CandidateID);&#xA;&#xA;DECLARE @dtProc as DATETIME&#xA;&#xA;select @dtProc = CONVERT(DATETIME,CD.DECODETXT)&#xA;FROM [2761_EnterpriseODS_Stage_Rel1].empref.CodeDetail CD WITH (NOLOCK)&#xA;JOIN [2761_EnterpriseODS_Stage_Rel1].empref.CodeHeader CH WITH (NOLOCK)&#xA;&#x9;on CH.CategoryNbr = CD.CategoryNbr&#xA;WHERE CH.CategoryDesc = 'EmailRmndrLastProcDt'&#xA;&#xA;INSERT INTO #varCandidate(CandidateID,CandidateSourceCd,HiredDt,CandidateIDDummy, PeopleKey,EnterpriseID,PersonnelNbr,firstnm,lastnm)&#xA;SELECT&#xA;&#x9;a.CandidateId,&#xA;&#x9;a.CandidateSourceCd,&#xA;&#x9;a.HiredDt,&#xA;&#x9;CASE &#xA;&#x9;&#x9;WHEN A.CandidateSourceCd = 'ATAS1' then a.CandidateID&#xA;&#x9;&#x9;WHEN A.CandidateSourceCd = 'AB-PH-SWF' then 'PDCA' + a.CandidateID&#xA;&#x9;&#x9;WHEN A.CandidateSourceCd = 'AB-PH-BPO' then 'PDCB' + a.CandidateID&#xA;&#x9;&#x9;WHEN A.CandidateSourceCd = 'AB-IN-SWF' then 'INDA' + a.CandidateID&#xA;&#x9;&#x9;WHEN A.CandidateSourceCd = 'AB-IN-BPO' then 'INDB' + a.CandidateID&#xA;&#x9;&#x9;WHEN A.CandidateSourceCd = 'AB-IN-ICO' then 'INDC' + a.CandidateID&#xA;&#x9;&#x9;WHEN A.CandidateSourceCd = 'AB-IN-EWF' then 'INDD' + a.CandidateID&#xA;&#x9;&#x9;ELSE '' END,&#xA;&#x9;ISNULL(b.PeopleKey,0),&#xA;&#x9;ISNULL(b.EnterpriseID,''),&#xA;--------Workday Update&#xA;&#x9;ISNULL(b.EmployeeId,''),&#xA;&#x9;ISNULL(b.firstnm,''),&#xA;&#x9;ISNULL(b.lastnm,'')&#xA;FROM [2761_EnterpriseODS_Rel1].EMPREF.CandidateStatus A WITH (NOLOCK)&#xA;JOIN [2761_EnterpriseODS_Rel1].EMPREF.Candidate B WITH (NOLOCK)&#xA;on B.CandidateId = a.CandidateId&#xA;and B.CandidateSourceCd = a.CandidateSourceCd&#xA;WHERE&#xA;&#x9;A.LastModifiedDttm &gt;= @dtProc&#xA;&#x9;AND A.ReferralStatusDesc in ('Hired', 'Onboard')&#xA;GROUP BY&#xA;&#x9;a.CandidateId,&#xA;&#x9;a.CandidateSourceCd,&#xA;&#x9;a.HiredDt,&#xA;&#x9;b.peoplekey,&#xA;&#x9;b.enterpriseid,&#xA;--------Workday Update&#xA;&#x9;b.EmployeeId,&#xA;&#x9;b.firstnm,&#xA;&#x9;b.lastnm&#xA;&#x9;&#xA;--Get People Data using firstnm, lastnm, and candidateid&#xA;UPDATE A&#xA;SET A.PeopleKey = PP.PeopleKey,&#xA;A.EnterpriseID = PP.EnterpriseID,&#xA;A.PersonnelNbr = PP.PersonnelNbr&#xA;FROM #varCandidate A WITH (NOLOCK)&#xA;JOIN [2761_EnterpriseODS_Rel1].[MRDRSensitive].[People] PP WITH (NOLOCK)&#xA;ON PP.candidateid like '%' + a.candidateid + '%'&#xA;and pp.firstnm = a.firstnm&#xA;and pp.lastnm = a.lastnm&#xA;WHERE (&#xA;&#x9;&#x9;pp.peoplekey &lt;&gt; ISNULL(a.peoplekey,0) or&#xA;&#x9;&#x9;pp.EnterpriseId &lt;&gt; ISNULL(a.EnterpriseId,'') or&#xA;&#x9;&#x9;pp.PersonnelNbr &lt;&gt; ISNULL(a.PersonnelNbr,'')&#xA;&#x9;)&#x9;&#xA;&#xA;--Get People Data using candidateid exact&#xA;UPDATE A&#xA;SET A.PeopleKey = PP.PeopleKey,&#xA;A.EnterpriseID = PP.EnterpriseID,&#xA;A.PersonnelNbr = PP.PersonnelNbr&#xA;FROM #varCandidate A WITH (NOLOCK)&#xA;JOIN [2761_EnterpriseODS_Rel1].[MRDRSensitive].[People] PP WITH (NOLOCK)&#xA;ON a.CandidateIDDummy = PP.CandidateID&#xA;where&#xA;&#x9;(&#xA;&#x9;&#x9;pp.peoplekey &lt;&gt; ISNULL(a.peoplekey,0) or&#xA;&#x9;&#x9;pp.EnterpriseId &lt;&gt; ISNULL(a.EnterpriseId,'') or&#xA;&#x9;&#x9;pp.PersonnelNbr &lt;&gt; ISNULL(a.PersonnelNbr,'')&#xA;&#x9;)&#xA;&#xA;--Update empref.Candidate table&#xA;UPDATE A&#xA;SET A.PeopleKey = b.PeopleKey,&#xA;--------Workday Update&#xA;a.EmployeeId = b.PersonnelNbr,&#xA;a.EnterpriseId = b.EnterpriseID,&#xA;a.UpdateDttm = GETDATE()&#xA;FROM [2761_EnterpriseODS_Rel1].EMPREF.Candidate A WITH (NOLOCK)&#xA;join #varCandidate B WITH (NOLOCK)&#xA;on b.CandidateID = a.CandidateID&#xA;and b.CandidateSourceCd = a.CandidateSourceCd&#xA;WHERE&#xA;&#x9;(&#xA;&#x9;&#x9;ISNULL(A.enterpriseid,'') &lt;&gt; isnull(b.enterpriseid, '') OR&#xA;&#x9;&#x9;ISNULL(A.peoplekey,0) &lt;&gt; isnull(b.peoplekey, 0) OR&#xA;&#x9;&#x9;ISNULL(A.enterpriseid,0) &lt;&gt; isnull(b.enterpriseid, 0)&#xA;&#x9;)&#xA;&#x9;and b.peoplekey &lt;&gt; 0&#xA;&#xA;--Get employees with updates&#xA;;&#xA;with&#xA;People (ActualCandidateID, CandidateID, PeopleKey, EnterpriseID, PersonnelNbr, CandidateSourceCd, FirstNm, LastNm)&#xA;as&#xA;(&#xA;select &#xA;CandidateID,&#xA;CASE WHEN SUBSTRING(CandidateID,1,3) = 'PDC' OR SUBSTRING(CandidateID,1,3) = 'IND'&#xA;&#x9;&#x9;THEN SUBSTRING(CandidateID,5, LEN(CandidateID))&#xA;&#x9;ELSE CandidateID END,&#xA;PeopleKey,&#xA;EnterpriseID,&#xA;PersonnelNbr,&#xA;CASE &#xA;&#x9;&#x9;WHEN SUBSTRING(P.CandidateID,1,4) ='PDCA' THEN 'AB-PH-SWF'&#xA;&#x9;&#x9;WHEN SUBSTRING(P.CandidateID,1,4) ='PDCB' THEN 'AB-PH-BPO'&#xA;&#x9;&#x9;WHEN SUBSTRING(P.CandidateID,1,4) ='INDA' THEN 'AB-IN-SWF'&#xA;&#x9;&#x9;WHEN SUBSTRING(P.CandidateID,1,4) ='INDB' THEN 'AB-IN-BPO'&#xA;&#x9;&#x9;WHEN SUBSTRING(P.CandidateID,1,4) ='INDC' THEN 'AB-IN-ICO'&#xA;&#x9;&#x9;WHEN SUBSTRING(P.CandidateID,1,4) ='INDD' THEN 'AB-IN-EWF'&#xA;&#x9;&#x9;ELSE 'ATAS1' END as CandidateSourceCd,&#xA;ISNULL(FirstNm,''),&#xA;ISNULL(LastNm,'')&#xA;from [2761_EnterpriseODS_Rel1].MRDRSensitive.[People] p with (nolock)&#xA;where SAPEffectiveDttm &gt;= @dtProc&#xA;and ISNULL(CandidateID,'') &lt;&gt; ''&#xA;)&#xA;insert into #varPeople&#xA;select CandidateID,ActualCandidateID, PeopleKey, EnterpriseID, PersonnelNbr, CandidateSourceCd, FirstNm, LastNm from People&#xA;group by &#xA;CandidateID,&#xA;ActualCandidateID,&#xA;PeopleKey,&#xA;EnterpriseID,&#xA;PersonnelNbr,&#xA;CandidateSourceCd,&#xA;FIrstNm,&#xA;LastNm&#xA;&#xA;--Update empref.Candidate based if there are updates in People table&#xA;UPDATE A&#xA;SET a.PeopleKey = b.PeopleKey,&#xA;a.EnterpriseID = b.EnterpriseID,&#xA;--------Workday Update&#xA;a.EmployeeId = b.PersonnelNbr,&#xA;a.UpdateDttm = getdate()&#xA;FROM [2761_EnterpriseODS_Rel1].EMPREF.Candidate A WITH (NOLOCK)&#xA;join [2761_EnterpriseODS_Rel1].EMPREF.CandidateStatus CS WITH (NOLOCK)&#xA;on A.CandidateID = CS.CandidateID&#xA;and A.CandidateSourceCd = CS.CandidateSourceCd&#xA;join #varPeople B WITH (NOLOCK)&#xA;on B.ActualCandidateID LIKE '%' + A.CandidateID + '%'&#xA;and b.FirstNm = a.FIrstNm&#xA;and b.LastNm = a.LastNm&#xA;WHERE &#xA;CS.ReferralStatusDesc in ('Hired', 'Onboard')&#xA;AND (&#xA;&#x9;ISNULL(A.PeopleKey,0) &lt;&gt; B.PeopleKey &#xA;&#x9;OR ISNULL(A.EnterpriseID,'') &lt;&gt; B.EnterpriseID &#xA;--------Workday Update&#xA;&#x9;OR ISNULL(A.EmployeeId,'') &lt;&gt; b.PersonnelNbr)&#xA;AND b.PeopleKey &lt;&gt; 0&#xA;&#xA;--Update empref.Candidate based if there are updates in People table&#xA;UPDATE A&#xA;SET a.PeopleKey = b.PeopleKey,&#xA;a.EnterpriseID = b.EnterpriseID,&#xA;--------Workday Update&#xA;a.EmployeeId = b.PersonnelNbr,&#xA;a.UpdateDttm = getdate()&#xA;FROM [2761_EnterpriseODS_Rel1].EMPREF.Candidate A WITH (NOLOCK)&#xA;join [2761_EnterpriseODS_Rel1].EMPREF.CandidateStatus CS WITH (NOLOCK)&#xA;on A.CandidateID = CS.CandidateID&#xA;and A.CandidateSourceCd = CS.CandidateSourceCd&#xA;join #varPeople B WITH (NOLOCK)&#xA;on b.CandidateID = a.CandidateID&#xA;and b.CandidateSourceCd = a.CandidateSourceCd&#xA;WHERE &#xA;CS.ReferralStatusDesc in ('Hired', 'Onboard')&#xA;AND (&#xA;&#x9;ISNULL(A.PeopleKey,0) &lt;&gt; B.PeopleKey &#xA;&#x9;OR ISNULL(A.EnterpriseID,'') &lt;&gt; B.EnterpriseID &#xA;--------Workday Update&#xA;&#x9;OR ISNULL(A.EmployeeId,'') &lt;&gt; b.PersonnelNbr)&#xA;AND b.PeopleKey &lt;&gt; 0&#xA;&#xA;UPDATE CD&#xA;SET CD.DECODETXT = CONVERT(DATE,DATEADD(DAY,-2,GETDATE()))&#xA;FROM [2761_EnterpriseODS_Stage_Rel1].empref.CodeDetail CD WITH (NOLOCK)&#xA;JOIN [2761_EnterpriseODS_Stage_Rel1].empref.CodeHeader CH WITH (NOLOCK)&#xA;&#x9;on CH.CategoryNbr = CD.CategoryNbr&#xA;WHERE CH.CategoryDesc = 'EmailRmndrLastProcDt'&#xA;&#xA;DROP TABLE #varCandidate&#xA;DROP TABLE #varPeople" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
